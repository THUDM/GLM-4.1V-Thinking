{
  "name": "vlm-helper",
  "version": "1.0.6",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:with-typecheck": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps && pnpm run rebuild",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/katex": "^0.16.7",
    "@types/markdown-it": "^14.1.2",
    "@vicons/carbon": "^0.13.0",
    "@vicons/tabler": "^0.13.0",
    "@vueuse/core": "^13.6.0",
    "better-sqlite3": "^12.2.0",
    "electron-screenshot": "^1.0.5",
    "ffmpeg-static-electron": "^2.0.3",
    "fluent-ffmpeg": "^2.1.3",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.22",
    "markdown-it": "^14.1.0",
    "markdown-it-highlightjs": "^4.2.0",
    "markdown-it-katex": "^2.0.3",
    "naive-ui": "^2.42.0",
    "pdfjs-dist": "^5.4.54",
    "pinia": "^3.0.3",
    "pptxtojson": "^1.5.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/carbon": "^1.2.11",
    "@iconify/types": "^2.0.0",
    "@types/node": "^22.16.5",
    "@unocss/preset-icons": "^66.3.3",
    "@unocss/preset-uno": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "vite": "^7.0.5",
    "vue": "^3.5.17",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "electron",
      "esbuild"
    ]
  }
}
